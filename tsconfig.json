{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "paths": {
      "shared/routes": ["./shared/routes"],
      "libs/api":["./libs/api"],
      "libs/ui-elements":["./libs/ui-elements"]
    },
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node"
  }
}
